<?xml version="1.0"?>
<openerp>
<data noupdate="1">

    <record id="opening_acta" model="ir.actions.report.xml">
        <field name="name">Opening Acta</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">project.project</field>
        <field name="report_name">opening_acta</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="report_rml">project_adhoc_personalization/report/acta_apertura.odt</field>
        <field name="out_format" ref="report_aeroo.report_mimetypes_pdf_odt"/>
        <field name="parser_state">default</field>
        <field name="tml_source">file</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['project.project']" name="models"/>
        <field name="name">Opening Acta</field>
        <field eval="'ir.actions.report.xml,'+str(opening_acta)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>

    <record id="close_acta" model="ir.actions.report.xml">
        <field name="name">Close Acta</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">project.project</field>
        <field name="report_name">close_acta</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="report_rml">project_adhoc_personalization/report/acta_cierre.odt</field>
        <field name="out_format" ref="report_aeroo.report_mimetypes_pdf_odt"/>
        <field name="parser_state">default</field>
        <field name="tml_source">file</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['project.project']" name="models"/>
        <field name="name">Close Acta</field>
        <field eval="'ir.actions.report.xml,'+str(close_acta)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>



</data>
</openerp>

