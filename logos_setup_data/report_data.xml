        <field eval="'ir.actions.report.xml,'+str(picking_labels_report)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>

    <!-- Catalogo adaptado a logos -->
<!-- ODS Catalog -->
