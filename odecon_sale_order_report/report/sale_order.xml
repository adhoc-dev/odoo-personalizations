<?xml version="1.0"?>
<openerp>
<data>
    
    <record id="sale_order_report" model="ir.actions.report.xml">
        <field name="name">Sale Order Report</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">sale.order</field>
        <field name="report_name">sale.sale_order</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="parser_loc">l10n_ar_aeroo_base/parser.py</field>
        <field name="parser_state">loc</field>        
        <field name="report_rml">odecon_sale_order_report/report/sale_order.odt</field>
        <field name="out_format" ref="report_aeroo.report_mimetypes_pdf_odt"/>
        <field name="tml_source">file</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['sale.order']" name="models"/>
        <field name="name">Sale Order Report</field>
        <field eval="'ir.actions.report.xml,'+str(sale_order_report)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>
    
    <record model="report.configuration.line" id="sale_force_code">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">force_code</field>
        <field name="value_type">text</field>
    </record> 
    <record model="report.configuration.line" id="sale_force_title">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">force_title</field>
        <field name="value_type">text</field>
    </record>

    <record model="report.configuration.line" id="sale_1">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">print_number</field>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="True"/>
    </record>

    <record model="report.configuration.line" id="sale_2">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">print_letter</field>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="True"/>
    </record> 

    <record model="report.configuration.line" id="sale_3">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">print_title</field>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="True"/>
    </record>

    <record model="report.configuration.line" id="sale_4">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">legend</field>
        <field name="value_text">Doc. no v√°lido como factura</field>
        <field name="value_type">text</field>
    </record>

    <record model="report.configuration.line" id="sale_5">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">print_commercial</field>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="True"/>
    </record>

    <record model="report.configuration.line" id="sale_7">
        <field name="report_id" ref="sale_order_report"/>
        <field name="name">print_page_number</field>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="True"/>
    </record> 
</data>
</openerp>